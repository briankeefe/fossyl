{
  "name": "fossyl",
  "version": "0.15.0",
  "type": "module",
  "description": "CLI for scaffolding fossyl projects",
  "bin": {
    "fossyl": "./bin/fossyl.js"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "bin",
    "dist",
    "CLAUDE.md"
  ],
  "scripts": {
    "build": "node scripts/generate-versions.js && tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "fossyl",
    "cli",
    "scaffolding",
    "rest-api",
    "typescript",
    "generator",
    "project-generator",
    "create-app",
    "starter",
    "boilerplate",
    "api-generator",
    "nodejs",
    "backend"
  ],
  "author": "YoyoSaur",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/YoyoSaur/fossyl.git",
    "directory": "packages/cli"
  },
  "homepage": "https://github.com/YoyoSaur/fossyl#readme",
  "bugs": {
    "url": "https://github.com/YoyoSaur/fossyl/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@clack/prompts": "^0.7.0"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.8.0"
  }
}
