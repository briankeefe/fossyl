{
  "name": "@fossyl/kysely",
  "version": "0.9.0",
  "description": "Kysely runtime database adapter for fossyl - type-safe queries, transactions, and migrations",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src",
    "CLAUDE.md",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "fossyl",
    "kysely",
    "typescript",
    "database",
    "type-safe",
    "adapter",
    "transactions",
    "migrations"
  ],
  "author": "YoyoSaur",
  "license": "GPL-3.0",
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "kysely": ">=0.27.0",
    "@fossyl/core": ">=0.1.0"
  },
  "devDependencies": {
    "@fossyl/core": "workspace:*",
    "kysely": "^0.27.0",
    "tsup": "^8.0.0",
    "typescript": "^5.8.3"
  }
}
