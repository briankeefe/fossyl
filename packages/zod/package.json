{
  "name": "@fossyl/zod",
  "version": "0.1.0",
  "description": "Zod validation adapter for fossyl",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src",
    "CLAUDE.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "fossyl",
    "zod",
    "validation",
    "typescript",
    "type-safe"
  ],
  "author": "YoyoSaur",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/YoyoSaur/fossyl.git",
    "directory": "packages/zod"
  },
  "homepage": "https://github.com/YoyoSaur/fossyl#readme",
  "bugs": {
    "url": "https://github.com/YoyoSaur/fossyl/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "zod": "^3.0.0"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.8.0",
    "zod": "^3.23.0"
  }
}
